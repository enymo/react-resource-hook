{
  "name": "@enymo/react-resource-hook",
  "version": "6.0.0-beta.1",
  "description": "React hook for interacting with REST APIs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.ts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "tsup": {
    "entry": [
      "src/js/index.tsx"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enymo/react-resource-hook.git"
  },
  "author": "enymo GmbH",
  "license": "MIT",
  "devDependencies": {
    "@types/pluralize": "^0.0.29",
    "@types/react": "^19.1.8",
    "postcss": "^8.4.17",
    "tsup": "^8.0.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@enymo/react-socket-hook": "^1.1.3",
    "react": ">=17.0.0"
  },
  "dependencies": {
    "@enymo/comparison": "^1.0.5",
    "@enymo/react-better-context": "^1.0.1",
    "@enymo/ts-nullsafe": "^1.1.1",
    "pluralize": "^8.0.0",
    "ts-essentials": "^10.1.1"
  }
}
